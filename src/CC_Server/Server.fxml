<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CC_Server.ServerController">
   <children>
      <TextArea fx:id="txtAreaServer" layoutX="14.0" layoutY="14.0" prefHeight="178.0" prefWidth="324.0" />
      <Label fx:id="loginLabel1" layoutX="354.0" layoutY="53.0" text="Username:" />
      <Label fx:id="logicLabel2" layoutX="359.0" layoutY="95.0" text="Password:" />
      <TextField fx:id="txtBoxUsername" layoutX="420.0" layoutY="49.0" />
      <TextField fx:id="txtBoxPassword" layoutX="422.0" layoutY="91.0" />
      <Button fx:id="btnAttemptConnection" layoutX="400.0" layoutY="160.0" mnemonicParsing="false" onAction="#btnConnection" text="Attempt Connection" />
      <Button fx:id="btnRange" layoutX="347.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnRange" text="Set Range" />
      <ComboBox fx:id="comboBoxRange" layoutX="484.0" layoutY="11.0" prefHeight="26.0" prefWidth="90.0" />
      <Button fx:id="btnVerifyDBIntegrity" layoutX="388.0" layoutY="124.0" mnemonicParsing="false" text="Verify Database Integrity" />
