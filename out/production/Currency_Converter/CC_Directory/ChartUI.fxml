<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CC_Directory.ChartController">
   <children>
      <LineChart fx:id="lineChart" animated="false" layoutX="11.0" layoutY="10.0" prefHeight="600.0" prefWidth="780.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="chartButton" layoutX="11.0" layoutY="614.0" mnemonicParsing="false" onAction="#btnAction" text="Chart Data" />
   </children>
</AnchorPane>
